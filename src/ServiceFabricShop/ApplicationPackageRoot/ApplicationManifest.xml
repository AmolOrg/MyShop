<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ApplicationTypeName="ServiceFabricShopType" ApplicationTypeVersion="1.0.0" xmlns="http://schemas.microsoft.com/2011/01/fabric">
  <Parameters>
    <Parameter Name="ShopFront_InstanceCount" DefaultValue="-1" />
    <Parameter Name="ProductsApi_InstanceCount" DefaultValue="-1" />
    <Parameter Name="RatingsApi_InstanceCount" DefaultValue="-1" />
    <Parameter Name="RecommandationsApi_InstanceCount" DefaultValue="-1" />
  </Parameters>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="ShopFrontPkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="ProductsApiPkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="RatingsApiPkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="RecommandationsApiPkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <DefaultServices>
    <Service Name="ShopFront">
      <StatelessService ServiceTypeName="ShopFrontType" InstanceCount="[ShopFront_InstanceCount]">
        <SingletonPartition />
      </StatelessService>
    </Service>
    <Service Name="ProductsApi">
      <StatelessService ServiceTypeName="ProductsApiType" InstanceCount="[ProductsApi_InstanceCount]">
        <SingletonPartition />
      </StatelessService>
    </Service>
    <Service Name="RatingsApi">
      <StatelessService ServiceTypeName="RatingsApiType" InstanceCount="[RatingsApi_InstanceCount]">
        <SingletonPartition />
      </StatelessService>
    </Service>
    <Service Name="RecommandationsApi">
      <StatelessService ServiceTypeName="RecommandationsApiType" InstanceCount="[RecommandationsApi_InstanceCount]">
        <SingletonPartition />
      </StatelessService>
    </Service>
  </DefaultServices>
</ApplicationManifest>